{
    "name": "muhamed-didovic/csrf",
    "type": "library",
    "description": "Protect your application from cross-site request forgery (CSRF) attacks",
    "keywords": [
        "muhamed-didovic",
        "csrf"
    ],
    "homepage": "https://github.com/muhamed-didovic/csrf",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhamed Didovic",
            "email": "muhamed.didovic@gmail.com",
            "homepage": "https://github.com/muhamed-didovic",
            "role": "Developer"
        },
        {
            "name": "Goran Radosevic",
            "email": "goran.radosevic@outlook.com"
        }
    ],
    "require": {
        "php" : ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "muhamed-didovic\\csrf\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "muhamed-didovic\\csrf\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
